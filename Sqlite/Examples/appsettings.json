    "DefaultTableName": "tinybase_store",
    "AutoLoadIntervalSeconds": 5,
    "CommandTimeout": 30,
    "EnableSqlLogging": true,
    "DatabaseProvider": "SQLite",
    "UseInMemory": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "TinyBaseSqlitePersister": "Debug"
    }
  }
}